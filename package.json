{
  "name": "crux-css",
  "version": "4.0.0",
  "description": "generate atomic css",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "crux": "bin/crux.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/.bin/tape ./test/*.spec.ts | tap-spec",
    "dist": "tsc",
    "example:node": "ts-node ./example/node.ts",
    "example:cli": "npm run dist && node ./bin/crux.js --variables=./example/cli.json -o ./example/cli.css",
    "deploy": "npm run dist && npm publish"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "@types/tape": "^4.2.29",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "argv": "0.0.2",
    "fs": "0.0.1-security"
  }
}
